{
  "name": "platform-backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prod": "NODE_ENV=production ts-node-dev --respawn --transpile-only src/index.ts",
    "provider:fix-count": "NODE_ENV=production ts-node-dev --respawn --transpile-only src/fixProviderPatientCount.ts",
    "provider:fix-assignment": "NODE_ENV=production ts-node-dev --respawn --transpile-only src/fixProviderPatientAssignment.ts",
    "format": "prettier --check . --config ./.prettierrc",
    "format:fix": "prettier --write . --config ./.prettierrc",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "tsc": "tsc --noEmit --project ./tsconfig.json",
    "prepare": "husky install && npx husky add .husky/pre-commit \"node_modules/.bin/lint-staged\"",
    "sendbird:sync": "ts-node ./scripts/synchronizeSendbird.ts"
  },
